<Window     
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:MetroWindows="clr-namespace:LeoBot" x:Class="LeoBot.MainWindow"
        AllowsTransparency="True" Title="LeoBot" Height="458.4" Width="1200" Style="{DynamicResource MetroWindowStyle}" 
        ResizeMode="NoResize" Topmost="False" WindowStartupLocation="CenterScreen">
    <Window.Resources>
        <Style x:Key="MetroWindowStyle" TargetType="{x:Type Window}">
            <Setter Property="Foreground" Value="{DynamicResource {x:Static SystemColors.WindowTextBrushKey}}"/>
            <Setter Property="Background" Value="{DynamicResource {x:Static SystemColors.WindowBrushKey}}"/>
            <Setter Property="WindowStyle" Value="None" />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type Window}">
                        <Grid>
                            <Border x:Name="MainBorder" BorderThickness="0" Background="White" >
                                <DockPanel LastChildFill="True">
                                    <Border x:Name="PART_TITLEBAR" 
        								Margin="0,0,0,0" Height="40" Width="1190" DockPanel.Dock="Top"
        								Background="Transparent" MouseLeftButtonDown="PART_TITLEBAR_MouseLeftButtonDown">
                                        <DockPanel LastChildFill="False">
                                            <TextBlock Margin="10,0,0,4" VerticalAlignment="Center"
        										Foreground="#FFAAAAAA" TextWrapping="NoWrap" 
        										Text="{TemplateBinding Title}" FontSize="18" />
                                            <Button x:Name="PART_CLOSE" DockPanel.Dock="Right" 
        										VerticalAlignment="Center" Margin="5"
        										Height="20" Width="20" 
        										Style="{DynamicResource MetroWindowButtonStyle}"
        										Click="PART_CLOSE_Click">
                                                <Path Data="F1M54.0573,47.8776L38.1771,31.9974 54.0547,16.1198C55.7604,14.4141 55.7604,11.6511 54.0573,9.94531 52.3516,8.23962 49.5859,8.23962 47.8802,9.94531L32.0026,25.8229 16.1224,9.94531C14.4167,8.23962 11.6511,8.23962 9.94794,9.94531 8.24219,11.6511 8.24219,14.4141 9.94794,16.1198L25.8255,32 9.94794,47.8776C8.24219,49.5834 8.24219,52.3477 9.94794,54.0534 11.6511,55.7572 14.4167,55.7585 16.1224,54.0534L32.0026,38.1745 47.8802,54.0534C49.5859,55.7585 52.3516,55.7572 54.0573,54.0534 55.7604,52.3477 55.763,49.5834 54.0573,47.8776z" 
        											Stretch="Uniform" Fill="#FFAAAAAA" Width="10" Margin="0,0,0,0" />
                                            </Button>
                                            <Button x:Name="PART_MINIMIZE" 
        										HorizontalAlignment="Center" 
        										VerticalAlignment="Center" Margin="5"
        										DockPanel.Dock="Right"
        										Height="20" Width="20" Style="{DynamicResource MetroWindowButtonStyle}"
        										Click="PART_MINIMIZE_Click" VerticalContentAlignment="Bottom">
                                                <Path Data="M0,20L53.333,20 53.333,8.888 0,8.888z" 
        											Stretch="Uniform" Fill="#FFAAAAAA" Width="10" Margin="0,0,0,5"/>
                                            </Button>
                                        </DockPanel>
                                    </Border>
                                    <AdornerDecorator DockPanel.Dock="Bottom">
                                        <ContentPresenter ContentTemplate="{TemplateBinding ContentTemplate}" Content="{TemplateBinding Content}" ContentStringFormat="{TemplateBinding ContentStringFormat}"/>
                                    </AdornerDecorator>
                                </DockPanel>
                            </Border>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
            <Style.Triggers>
                <Trigger Property="ResizeMode" Value="CanResizeWithGrip">
                    <Setter Property="Template">
                        <Setter.Value>
                            <ControlTemplate TargetType="{x:Type Window}">
                                <Border BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Background="{TemplateBinding Background}">
                                    <Grid>
                                        <AdornerDecorator>
                                            <ContentPresenter ContentTemplate="{TemplateBinding ContentTemplate}" Content="{TemplateBinding Content}" ContentStringFormat="{TemplateBinding ContentStringFormat}"/>
                                        </AdornerDecorator>
                                        <ResizeGrip x:Name="WindowResizeGrip" HorizontalAlignment="Right" IsTabStop="False" Visibility="Collapsed" VerticalAlignment="Bottom"/>
                                    </Grid>
                                </Border>
                                <ControlTemplate.Triggers>
                                    <MultiTrigger>
                                        <MultiTrigger.Conditions>
                                            <Condition Property="ResizeMode" Value="CanResizeWithGrip"/>
                                            <Condition Property="WindowState" Value="Normal"/>
                                        </MultiTrigger.Conditions>
                                        <Setter Property="Visibility" TargetName="WindowResizeGrip" Value="Visible"/>
                                    </MultiTrigger>
                                </ControlTemplate.Triggers>
                            </ControlTemplate>
                        </Setter.Value>
                    </Setter>
                </Trigger>
            </Style.Triggers>
        </Style>
        <Style x:Key="MetroWindowButtonStyle" TargetType="{x:Type Button}">
            <Setter Property="FocusVisualStyle">
                <Setter.Value>
                    <Style>
                        <Setter Property="Control.Template">
                            <Setter.Value>
                                <ControlTemplate>
                                    <Rectangle Margin="4" SnapsToDevicePixels="True" Stroke="{DynamicResource {x:Static SystemColors.ControlTextBrushKey}}" StrokeThickness="1" StrokeDashArray="1 2"/>
                                </ControlTemplate>
                            </Setter.Value>
                        </Setter>
                    </Style>
                </Setter.Value>
            </Setter>
            <Setter Property="Background" Value="{x:Null}"/>
            <Setter Property="BorderBrush" Value="#FF707070"/>
            <Setter Property="Foreground" Value="{DynamicResource {x:Static SystemColors.ControlTextBrushKey}}"/>
            <Setter Property="BorderThickness" Value="0"/>
            <Setter Property="HorizontalContentAlignment" Value="Center"/>
            <Setter Property="VerticalContentAlignment" Value="Center"/>
            <Setter Property="Padding" Value="1"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type Button}">
                        <Border x:Name="border" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Background="{TemplateBinding Background}" SnapsToDevicePixels="True">
                            <ContentPresenter x:Name="contentPresenter" ContentTemplate="{TemplateBinding ContentTemplate}" Content="{TemplateBinding Content}" ContentStringFormat="{TemplateBinding ContentStringFormat}" Focusable="False" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" Margin="{TemplateBinding Padding}" RecognizesAccessKey="True" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                        </Border>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsDefaulted" Value="True">
                                <Setter Property="BorderBrush" TargetName="border" Value="{DynamicResource {x:Static SystemColors.HighlightBrushKey}}"/>
                            </Trigger>
                            <Trigger Property="IsMouseOver" Value="True">
                                <Setter Property="Background" TargetName="border" Value="#FFBEE6FD"/>
                                <Setter Property="BorderBrush" TargetName="border" Value="#FF3C7FB1"/>
                            </Trigger>
                            <Trigger Property="IsPressed" Value="True">
                                <Setter Property="Background" TargetName="border" Value="#FFC4E5F6"/>
                                <Setter Property="BorderBrush" TargetName="border" Value="#FF2C628B"/>
                            </Trigger>
                            <Trigger Property="IsEnabled" Value="False">
                                <Setter Property="Background" TargetName="border" Value="#FFF4F4F4"/>
                                <Setter Property="BorderBrush" TargetName="border" Value="#FFADB2B5"/>
                                <Setter Property="TextElement.Foreground" TargetName="contentPresenter" Value="#FF838383"/>
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
    </Window.Resources>
    <Grid Height="415">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="265*"/>
            <ColumnDefinition Width="336*"/>
            <ColumnDefinition Width="334*"/>
            <ColumnDefinition Width="265*"/>
        </Grid.ColumnDefinitions>
        <GroupBox Header="Character Info" HorizontalAlignment="Left" Height="408" Margin="10,0,0,0" VerticalAlignment="Top" Width="250" FontSize="14" FontWeight="Bold">
            <Grid HorizontalAlignment="Left" Height="410" Margin="0,3,-3.2,-30.6" VerticalAlignment="Top" Width="240">
                <Label Content="Account ID:" HorizontalAlignment="Left" Margin="4,2,0,378" VerticalAlignment="Center" FontSize="13" FontWeight="Normal" Height="30"/>
                <TextBox x:Name="accID" MaxLines="1" HorizontalAlignment="Left" Height="20" Margin="123,7,0,383" TextWrapping="NoWrap" VerticalAlignment="Center" Width="107" FontSize="13" FontWeight="Normal" IsReadOnly="False"/>
                <Label Content="Character ID:" HorizontalAlignment="Left" Margin="4,27,0,353" VerticalAlignment="Center" FontSize="13" FontWeight="Normal" Height="30"/>
                <TextBox x:Name="charID" MaxLines="1" HorizontalAlignment="Left" Height="20" Margin="123,32,0,358" TextWrapping="NoWrap" VerticalAlignment="Center" Width="107" FontSize="13" FontWeight="Normal" IsReadOnly="True"/>
                <Label Content="Character Name:" HorizontalAlignment="Left" Margin="4,52,0,328" VerticalAlignment="Center" FontSize="13" Height="30" FontWeight="Normal" Width="114"/>
                <TextBox x:Name="charname" MaxLines="1" HorizontalAlignment="Left" Height="20" Margin="123,57,0,333" TextWrapping="NoWrap" VerticalAlignment="Center" Width="107" FontSize="13" FontWeight="Normal" IsReadOnly="True"/>
                <Label Content="Berserk:" HorizontalAlignment="Left" Margin="4,75,0,301" VerticalAlignment="Center" FontSize="13" FontWeight="Normal" Height="30"/>
                <TextBox x:Name="berserklevel" MaxLines="1" HorizontalAlignment="Left" Height="20" Margin="67,80,0,306" TextWrapping="NoWrap" VerticalAlignment="Center" Width="27" FontSize="13" FontWeight="Normal" IsReadOnly="True"/>
                <ProgressBar x:Name="HP_Bar" HorizontalAlignment="Left" Height="20" Margin="123,110,0,0" VerticalAlignment="Top" Width="107" Foreground="#FFD31111"/>
                <Label Content="Current HP:" HorizontalAlignment="Left" Margin="4,105,0,274" VerticalAlignment="Center" FontSize="13" Height="30" FontWeight="Normal"/>
                <ProgressBar x:Name="MP_Bar" HorizontalAlignment="Left" Height="20" Margin="123,135,0,0" VerticalAlignment="Top" Width="107" Foreground="#FF0D33D0"/>
                <Label Content="Current MP:" HorizontalAlignment="Left" Margin="4,130,0,249" VerticalAlignment="Center" FontSize="13" Height="30" FontWeight="Normal"/>
                <Label Content="Status:" HorizontalAlignment="Left" Margin="100,75,0,301" VerticalAlignment="Center" Height="30" FontSize="13" FontWeight="Normal"/>
                <TextBox x:Name="status" MaxLines="1" HorizontalAlignment="Left" Height="20" Margin="156,80,0,306" TextWrapping="NoWrap" VerticalAlignment="Center" Width="74" FontSize="13" FontWeight="Normal" IsReadOnly="True"/>
                <Label Content="Grab Pet ID:" HorizontalAlignment="Left" Margin="4,160,0,215" VerticalAlignment="Center" Height="30" FontSize="13" FontWeight="Normal"/>
                <TextBox x:Name="grabid" MaxLines="1" HorizontalAlignment="Left" Height="20" Margin="123,165,0,220" TextWrapping="NoWrap" VerticalAlignment="Center" Width="107" FontSize="13" FontWeight="Normal" IsReadOnly="True"/>
                <Label Content="Attack Pet Name:" HorizontalAlignment="Left" Margin="4,237,0,143" VerticalAlignment="Center" FontSize="13" FontWeight="Normal" Height="30"/>
                <TextBox x:Name="grabname" MaxLines="1" HorizontalAlignment="Left" Height="20" Margin="123,192,0,198" TextWrapping="NoWrap" VerticalAlignment="Center" Width="107" FontSize="13" FontWeight="Normal" IsReadOnly="True"/>
                <Label Content="Attack Pet ID:" HorizontalAlignment="Left" Margin="4,212,0,168" VerticalAlignment="Center" FontSize="13" FontWeight="Normal" Height="30"/>
                <TextBox x:Name="attackid" MaxLines="1" HorizontalAlignment="Left" Height="20" Margin="123,217,0,173" TextWrapping="NoWrap" VerticalAlignment="Center" Width="107" FontSize="13" FontWeight="Normal" IsReadOnly="True"/>
                <Label Content="Grab Pet Name:" HorizontalAlignment="Left" Margin="4,187,0,193" VerticalAlignment="Center" FontSize="13" FontWeight="Normal" Height="30"/>
                <TextBox x:Name="attackname" MaxLines="1" HorizontalAlignment="Left" Height="20" Margin="123,242,0,148" TextWrapping="NoWrap" VerticalAlignment="Center" Width="107" FontSize="13" FontWeight="Normal" IsReadOnly="True"/>
                <ProgressBar x:Name="PetHP_Bar" HorizontalAlignment="Left" Height="20" Margin="123,268,0,0" VerticalAlignment="Top" Width="107" Foreground="#FFEB1616"/>
                <Label Content="Attack Pet HP:" HorizontalAlignment="Left" Margin="4,264,0,116" VerticalAlignment="Center" FontSize="13" FontWeight="Normal" Height="30"/>
                <ProgressBar x:Name="PetHGP_Bar" HorizontalAlignment="Left" Height="20" Margin="123,293,0,0" VerticalAlignment="Top" Width="107" Foreground="#FFF8CD03"/>
                <Label Content="Horse HP:" HorizontalAlignment="Left" Margin="4,351,0,29" VerticalAlignment="Center" FontSize="13" FontWeight="Normal" Height="30"/>
                <ProgressBar x:Name="HorseHP_Bar" HorizontalAlignment="Left" Height="20" Margin="123,355,0,0" VerticalAlignment="Top" Width="107" Foreground="#FFEB1616"/>
                <Label Content="Attack Pet HGP:" HorizontalAlignment="Left" Margin="4,289,0,91" VerticalAlignment="Center" FontSize="13" FontWeight="Normal" Height="30"/>
                <Label Content="Horse ID:" HorizontalAlignment="Left" Margin="4,326,0,54" VerticalAlignment="Center" FontSize="13" FontWeight="Normal" Height="30"/>
                <TextBox x:Name="horseid" HorizontalAlignment="Left" MaxLines="1" Height="20" Margin="123,330,0,60" TextWrapping="NoWrap" VerticalAlignment="Center" Width="107" FontSize="13" FontWeight="Normal" IsReadOnly="True"/>
            </Grid>
        </GroupBox>
        <GroupBox Header="Statistic" HorizontalAlignment="Left" Height="347" Margin="5.8,0,0,0" VerticalAlignment="Top" Width="250" Grid.Column="3" FontSize="14" FontWeight="Bold">
            <Grid HorizontalAlignment="Left" Height="410" Margin="0,3,-3.2,-91.4" VerticalAlignment="Top" Width="240">
                <Label Content="Gold Amount:" HorizontalAlignment="Left" Margin="4,3,0,377" VerticalAlignment="Center" FontSize="13" FontWeight="Normal" Height="30"/>
                <TextBox x:Name="gold" MaxLines="1" HorizontalAlignment="Left" Height="20" Margin="123,5,0,381" TextWrapping="NoWrap" VerticalAlignment="Center" Width="107" FontSize="13" FontWeight="Normal" IsReadOnly="True"/>
                <Label Content="Gold Difference:" HorizontalAlignment="Left" Margin="4,27,0,353" VerticalAlignment="Center" FontSize="13" FontWeight="Normal" Height="30"/>
                <TextBox x:Name="gold_difference" MaxLines="1" HorizontalAlignment="Left" Height="20" Margin="123,30,0,357" TextWrapping="NoWrap" VerticalAlignment="Center" Width="107" FontSize="13" FontWeight="Normal" IsReadOnly="True"/>
                <Label Content="Level:" HorizontalAlignment="Left" Margin="4,54,0,326" VerticalAlignment="Center" FontSize="13" FontWeight="Normal" Height="30"/>
                <TextBox x:Name="level" MaxLines="1" HorizontalAlignment="Left" Height="20" Margin="54,58,0,332" TextWrapping="NoWrap" VerticalAlignment="Center" Width="40" FontSize="13" FontWeight="Normal" IsReadOnly="True"/>
                <Label Content="Current EXP:" HorizontalAlignment="Left" Margin="4,83,0,297" VerticalAlignment="Center" FontSize="13" FontWeight="Normal" Height="30"/>
                <Label Content="Current SP:" HorizontalAlignment="Left" Margin="4,108,0,272" VerticalAlignment="Center" FontSize="13" FontWeight="Normal" Height="30"/>
                <Label Content="EXP:" HorizontalAlignment="Left" Margin="138,54,0,326" VerticalAlignment="Center" FontSize="13" FontWeight="Normal" Height="30"/>
                <TextBox x:Name="exppercent" MaxLines="1" HorizontalAlignment="Left" Height="20" Margin="180,58,0,332" TextWrapping="NoWrap" VerticalAlignment="Center" Width="50" FontSize="13" FontWeight="Normal" IsReadOnly="True"/>
                <Label Content="SP Gain:" HorizontalAlignment="Left" Margin="4,133,0,247" VerticalAlignment="Center" FontSize="13" FontWeight="Normal" Height="30"/>
                <TextBox x:Name="spgainedtotal" MaxLines="1" HorizontalAlignment="Left" Height="20" Margin="138,137,0,253" TextWrapping="NoWrap" VerticalAlignment="Center" Width="92" FontSize="13" FontWeight="Normal" IsReadOnly="True"/>
                <Label Content="Mobs Killed:" HorizontalAlignment="Left" Margin="4,208,0,172" VerticalAlignment="Center" FontSize="13" FontWeight="Normal" Height="30"/>
                <TextBox x:Name="spgainedpermin" MaxLines="1" HorizontalAlignment="Left" Height="20" Margin="138,162,0,228" TextWrapping="NoWrap" VerticalAlignment="Center" Width="92" FontSize="13" FontWeight="Normal" IsReadOnly="True"/>
                <Label Content="SP Gain/ Hour:" HorizontalAlignment="Left" Margin="4,183,0,197" VerticalAlignment="Center" FontSize="13" FontWeight="Normal" Height="30"/>
                <TextBox x:Name="spgainedperhour" MaxLines="1" HorizontalAlignment="Left" Height="20" Margin="138,187,0,203" TextWrapping="NoWrap" VerticalAlignment="Center" Width="92" FontSize="13" FontWeight="Normal" IsReadOnly="True"/>
                <Label Content="SP Gain/ Minute:" HorizontalAlignment="Left" Margin="4,157,0,223" VerticalAlignment="Center" FontSize="13" FontWeight="Normal" Height="30"/>
                <TextBox x:Name="mobkilled" MaxLines="1" HorizontalAlignment="Left" Height="20" Margin="138,212,0,178" TextWrapping="NoWrap" VerticalAlignment="Center" Width="92" FontSize="13" FontWeight="Normal" IsReadOnly="True"/>
                <Label Content="Died:" HorizontalAlignment="Left" Margin="4,233,0,147" VerticalAlignment="Center" FontSize="13" FontWeight="Normal" Height="30"/>
                <Label Content="Returning:" HorizontalAlignment="Left" Margin="4,258,0,122" VerticalAlignment="Center" FontSize="13" FontWeight="Normal" Height="30"/>
                <Label Content="Total time:" HorizontalAlignment="Left" Margin="4,287,0,93" VerticalAlignment="Center" FontSize="13" FontWeight="Normal" Height="30"/>
                <TextBox x:Name="currentexp" MaxLines="1" HorizontalAlignment="Left" Height="20" Margin="123,86,0,304" TextWrapping="NoWrap" VerticalAlignment="Center" Width="107" FontSize="13" FontWeight="Normal" IsReadOnly="True"/>
                <TextBox x:Name="currentsp" MaxLines="1" HorizontalAlignment="Left" Height="20" Margin="123,112,0,278" TextWrapping="NoWrap" VerticalAlignment="Center" Width="107" FontSize="13" FontWeight="Normal" IsReadOnly="True"/>
                <Label x:Name="timecount" Content="0 : 0 : 0" HorizontalAlignment="Left" Margin="134,287,0,93" VerticalAlignment="Center" FontSize="13" FontWeight="Normal" Height="30"/>
                <TextBox x:Name="diedcount" MaxLines="1" HorizontalAlignment="Left" Height="20" Margin="138,237,0,153" TextWrapping="NoWrap" VerticalAlignment="Center" Width="92" FontSize="13" FontWeight="Normal" IsReadOnly="True"/>
                <TextBox x:Name="returncount" MaxLines="1" HorizontalAlignment="Left" Height="20" Margin="138,262,0,128" TextWrapping="NoWrap" VerticalAlignment="Center" Width="92" FontSize="13" FontWeight="Normal" IsReadOnly="True"/>
            </Grid>
        </GroupBox>
        <TabControl Grid.ColumnSpan="2" HorizontalAlignment="Left" Height="367" VerticalAlignment="Top" Width="670" Grid.Column="1" Margin="0.2,1,0,0">
            <TabItem Header="Auto Login" FontSize="13">
                <Grid Background="White">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="279*"/>
                        <ColumnDefinition Width="386*"/>
                    </Grid.ColumnDefinitions>
                    <GroupBox Grid.Column="1" Header="Log" HorizontalAlignment="Left" Height="337" Margin="108.8,0,-0.2,-0.2" VerticalAlignment="Top" Width="277" FontWeight="Bold" FontSize="12">
                        <TextBox x:Name="logs" IsReadOnly="True" ScrollViewer.VerticalScrollBarVisibility="Auto" Height="312" Margin="0,2,-3,-2.8" TextWrapping="Wrap" VerticalAlignment="Top" FontWeight="Normal" Background="{DynamicResource {x:Static SystemColors.MenuBarBrushKey}}" FontSize="11"/>
                    </GroupBox>
                    <GroupBox Grid.ColumnSpan="2" Header="Login Credentials" HorizontalAlignment="Left" Height="337" Margin="0,0,0,-0.2" VerticalAlignment="Top" Width="388" FontWeight="Bold" FontSize="12">
                        <Grid Background="White">
                            <Label Content="Username:" HorizontalAlignment="Left" Margin="14,9,0,0" VerticalAlignment="Top" FontWeight="Normal"/>
                            <TextBox x:Name="username" HorizontalAlignment="Left" Height="20" Margin="85,13,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="90" FontWeight="Normal"/>
                            <Label Content="Password:" HorizontalAlignment="Left" Margin="14,34,0,0" VerticalAlignment="Top" FontWeight="Normal"/>
                            <PasswordBox x:Name="password" HorizontalAlignment="Left" Height="20" Margin="85,38,0,0" VerticalAlignment="Top" Width="90" FontWeight="Normal"/>
                            <GroupBox Header="Captcha" HorizontalAlignment="Left" Height="106" Margin="182,1,0,0" VerticalAlignment="Top" Width="191" FontStyle="Italic">
                                <Grid Background="White">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="13*"/>
                                        <ColumnDefinition Width="152*"/>
                                        <ColumnDefinition Width="13*"/>
                                    </Grid.ColumnDefinitions>
                                    <Image x:Name="captcha" HorizontalAlignment="Left" Height="45" Margin="0.2,10,0,0" VerticalAlignment="Top" Width="152" Grid.Column="1" Grid.ColumnSpan="2"/>
                                    <TextBox x:Name="capans" HorizontalAlignment="Left" Height="20" Margin="10,60,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="90" FontWeight="Normal" Grid.ColumnSpan="2" FontStyle="Normal"/>
                                </Grid>
                            </GroupBox>
                            <Button x:Name="sendcapans" Content="Send" Click="CaptchaAns" HorizontalAlignment="Left" Margin="296,78,0,0" VerticalAlignment="Top" Width="62" FontWeight="Normal" Background="{DynamicResource {x:Static SystemColors.ControlBrushKey}}" Height="20"/>
                            <Label Content="Servers:" HorizontalAlignment="Left" Margin="14,112,0,0" VerticalAlignment="Top" FontWeight="Normal"/>
                            <ComboBox x:Name="serverlist" HorizontalAlignment="Left" Height="22" Margin="123,115,0,0" VerticalAlignment="Top" Width="120" FontWeight="Normal"/>
                            <Label Content="Character's name:" HorizontalAlignment="Left" Margin="14,138,0,0" VerticalAlignment="Top" FontWeight="Normal"/>
                            <CheckBox x:Name="autologin" Content="Activate auto login" Click="AutoLoginCheck" HorizontalAlignment="Left" Margin="14,73,0,0" VerticalAlignment="Top" FontWeight="Normal"/>
                            <TextBox x:Name="charnameselect" HorizontalAlignment="Left" Height="20" Margin="123,142,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" FontWeight="Normal"/>
                            <CheckBox x:Name="characterselect" Click="AutoSelectCheck" Content="Auto select character" HorizontalAlignment="Left" Margin="14,180,0,0" VerticalAlignment="Top" FontWeight="Normal"/>
                            <ListBox x:Name="charlist" HorizontalAlignment="Left" Height="106" Margin="182,180,0,0" VerticalAlignment="Top" Width="186" FontWeight="Normal"/>
                            <Label Content="List of Characters" HorizontalAlignment="Left" Margin="266,154,0,0" VerticalAlignment="Top" FontWeight="Normal" FontStyle="Italic"/>
                            <Button x:Name="selectcharbutton" Content="Select a character" Click="SelectCharacter" Height="22" HorizontalAlignment="Left" Margin="182,291,0,0" VerticalAlignment="Top" Width="186" Background="{DynamicResource {x:Static SystemColors.ControlBrushKey}}" FontWeight="Normal"/>
                        </Grid>
                    </GroupBox>
                </Grid>
            </TabItem>
            <TabItem Header="Training" FontSize="13">
                <Grid Background="White">
                    <GroupBox Header="Training Area" HorizontalAlignment="Left" Height="203" VerticalAlignment="Top" Width="323" FontWeight="Bold" FontSize="12">
                        <TabControl FontWeight="Normal" Margin="0,6,0.2,1.2">
                            <TabItem Header="Walk around" FontSize="12" FontStyle="Italic">
                                <Grid Background="White" Margin="0,-2,-1.2,0" Height="140" VerticalAlignment="Top" HorizontalAlignment="Left" Width="306">
                                    <Label Content="Center X:" Height="30" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="17,10,0,0" FontWeight="Normal" FontSize="12" FontStyle="Normal"/>
                                    <Label Content="Center Y:" Height="30" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="153,10,0,0" FontWeight="Normal" FontSize="12" FontStyle="Normal"/>
                                    <TextBox x:Name="x_setbox" HorizontalAlignment="Left" Height="20" Margin="80,14,0,106" TextWrapping="NoWrap" VerticalAlignment="Center" Width="60" FontSize="12" FontWeight="Normal" IsReadOnly="True" MaxLines="1" FontStyle="Normal"/>
                                    <TextBox x:Name="y_setbox" HorizontalAlignment="Left" Height="20" Margin="216,14,0,106" TextWrapping="NoWrap" VerticalAlignment="Center" Width="60" FontSize="12" FontWeight="Normal" IsReadOnly="True" MaxLines="1" FontStyle="Normal"/>
                                    <Label Content="Training range:" Height="30" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,41,0,0" FontWeight="Normal" FontSize="12" FontStyle="Normal"/>
                                    <TextBox MaxLines="1" x:Name="trainingrangebox" Text="{Binding Value, ElementName=rangebar, StringFormat=\{0:0\}, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}" HorizontalAlignment="Left" Height="20" Margin="105,46,0,74" TextWrapping="NoWrap" VerticalAlignment="Center" Width="30" FontSize="12" FontWeight="Normal" IsReadOnly="False" FontStyle="Normal"/>
                                    <Slider x:Name="rangebar" HorizontalAlignment="Left" Height="20" Margin="10,76,0,0" VerticalAlignment="Top" Width="137" Maximum="200" SmallChange="10" Value="100" Interval="100"/>
                                    <Button Content="Set current position as training center" Click="SetCoordinate" HorizontalAlignment="Left" Height="30" Margin="10,106,0,0" VerticalAlignment="Top" Width="284" Background="{DynamicResource {x:Static SystemColors.ControlBrushKey}}" FontWeight="Normal" FontSize="12" FontStyle="Normal"/>
                                    <Label Content="Running range:" Height="30" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="152,41,0,0" FontWeight="Normal" FontSize="12" FontStyle="Normal"/>
                                    <TextBox MaxLines="1" x:Name="runningrangebox" Text="{Binding Value, ElementName=rangebar1, StringFormat=\{0:0\}, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}" HorizontalAlignment="Left" Height="20" Margin="248,46,0,74" TextWrapping="NoWrap" VerticalAlignment="Center" Width="30" FontSize="12" FontWeight="Normal" IsReadOnly="False" FontStyle="Normal"/>
                                    <Slider x:Name="rangebar1" HorizontalAlignment="Left" Height="20" Margin="154,76,0,0" VerticalAlignment="Top" Width="137" Maximum="200" SmallChange="10" Value="50" Interval="100"/>
                                </Grid>
                            </TabItem>
